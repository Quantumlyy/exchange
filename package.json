{
	"name": "@shibuidao/exchange",
	"version": "1.1.2",
	"repository": "https://github.com/shibuidao/exchange.git",
	"author": "Nejc Drobnic <yo@quantumly.dev>",
	"license": "UNLICENSED",
	"types": "typechain/index.ts",
	"main": "typechain/index.ts",
	"private": false,
	"scripts": {
		"suite": "concurrently -n node,tests --hide 0 --kill-others-on-fail 'npm:node:hardhat' 'npm:suite:_init'",
		"suite:_init": "yarn compile:localh:hardhat && yarn node:_delay && yarn suite:_action && yarn suite:_end",
		"suite:_action": "yarn test:localh:_init",
		"suite:_end": "node -e \"process.exit(69)\"",
		"node:hardhat": "hardhat node",
		"node:dapp": "dapp testnet",
		"node:_delay": "sleep 5",
		"typechain": "concurrently -n node,types --hide 0 --kill-others-on-fail 'npm:node:hardhat' 'npm:typechain:_action'",
		"typechain:_action": "yarn compile:localh:hardhat && yarn node:_delay && yarn typechain:_end",
		"typechain:_end": "node -e \"process.exit(69)\"",
		"compile": "yarn compile:localh:hardhat",
		"compile:localh:hardhat": "hardhat compile --network localh",
		"test:localh:_init": "yarn test:localh:hardhat && yarn test:localh:hardhat:deploy && yarn test:localh:dapp",
		"test:localh:hardhat": "hardhat test --network localh",
		"test:localh:hardhat:deploy": "hardhat run ./scripts/tests/proxyDeploy.ts --network localh",
		"test:localh:dapp": "dapp test --ffi --rpc",
		"sversion": "standard-version"
	},
	"files": [
		"typechain",
		"abis"
	],
	"dependencies": {},
	"devDependencies": {
		"@nomiclabs/hardhat-ethers": "^2.0.2",
		"@nomiclabs/hardhat-etherscan": "^2.1.6",
		"@nomiclabs/hardhat-solhint": "^2.0.0",
		"@nomiclabs/hardhat-waffle": "^2.0.1",
		"@openzeppelin/contracts": "^4.0.0",
		"@openzeppelin/contracts-upgradeable": "^4.0.0",
		"@openzeppelin/hardhat-upgrades": "^1.6.0",
		"@openzeppelin/test-helpers": "^0.5.10",
		"@primitivefi/hardhat-dodoc": "^0.1.3",
		"@rari-capital/solmate": "^6.2.0",
		"@sapphire/eslint-config": "^3.2.3",
		"@sapphire/prettier-config": "^1.1.6",
		"@sapphire/ts-config": "^3.0.0",
		"@typechain/ethers-v5": "^7.1.2",
		"@typechain/hardhat": "^2.3.0",
		"@types/chai": "^4.2.16",
		"@types/dotenv": "^8.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.9.6",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"@typescript-eslint/parser": "^4.31.2",
		"chai": "^4.3.4",
		"concurrently": "^7.0.0",
		"dotenv": "^10.0.0",
		"eslint": "^7.32.0",
		"ethereum-waffle": "^3.3.0",
		"ethers": "^5.3.0",
		"hardhat": "^2.3.0",
		"hardhat-abi-exporter": "^2.7.2",
		"hardhat-gas-reporter": "^1.0.4",
		"hardhat-tracer": "^1.0.0-alpha.6",
		"mocha": "^9.1.1",
		"nyc": "^15.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-solidity": "^1.0.0-beta.19",
		"pretty-quick": "^3.1.1",
		"solhint-plugin-prettier": "^0.0.5",
		"solidity-coverage": "^0.7.16",
		"standard-version": "^9.3.2",
		"ts-node": "^10.0.0",
		"typechain": "^5.1.2",
		"typescript": "^4.3.2",
		"zod": "^3.9.5"
	},
	"resolutions": {
		"colors": "1.4.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
